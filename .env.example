# Copy this file to .env and fill in the values that match your deployment.
# At least one provider (OpenAI, Azure OpenAI, or LangChain) should be
# configured so the CLI and library can authenticate.

# OpenAI SDK
OPENAI_API_KEY="sk-your-openai-api-key"
OPENAI_API_BASE="https://api.openai.com/v1"
OPENAI_MODEL="gpt-4o-mini"

# Azure OpenAI (optional)
AZURE_OPENAI_API_KEY="your-azure-openai-key"
AZURE_OPENAI_ENDPOINT="https://your-resource.openai.azure.com"
AZURE_OPENAI_API_VERSION="2024-02-01"
AZURE_OPENAI_DEPLOYMENT="gpt-4o-mini"

# LangChain / LangSmith telemetry (optional)
LANGCHAIN_API_KEY="your-langchain-key"
LANGCHAIN_TRACING_V2="false"
